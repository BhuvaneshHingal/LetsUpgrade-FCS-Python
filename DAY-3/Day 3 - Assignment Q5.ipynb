    "S = input()\n",
    "x = 0\n",
    "y = 0\n",
    "for i, indx in enumerate(S):\n",
    "    if (S[i]== 'L'):\n",
    "        x -= 1\n",
    "    elif (S[i]=='R'):\n",
    "        x += 1\n",
    "    elif (S[i]=='U'):\n",
    "        y += 1\n",
    "    elif (S[i]=='D'):\n",
    "        y -= 1\n",
    "    print(\"%d %d\"%(x,y))"
